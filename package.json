    "pg": "^8.12.0",
    "prisma-extension-pagination": "^0.7.4",
    "reflect-metadata": "^0.1.14",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "systeminformation": "^5.23.4",
    "tesseract.js": "^5.1.0",
