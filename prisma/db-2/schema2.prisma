generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma-db-2/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_2")
}

model AnalysisTaskStatus {
  id             Int @id @default(autoincrement())
  status         Int
  analysisTaskId Int
}

model FarseAction {
  id             Int       @id @default(autoincrement())
  tageNumber     String
  filePath       String
  parseResult    Json?     @db.Json
  remark         String?
  createdAt      DateTime? @db.Timestamptz(6)
  updatedAt      DateTime? @db.Timestamptz(6)
  factoryId      Int
  analysisTaskId Int
}

model FarseValue {
  id             Int       @id @default(autoincrement())
  tageNumber     String
  checkTime      DateTime  @db.Date
  parseValue     String?
  parseUnit      String?
  remark         String?
  createdAt      DateTime? @db.Timestamptz(6)
  updatedAt      DateTime? @db.Timestamptz(6)
  keywordId      Int
  unitId         Int?
  factoryId      Int
  analysisTaskId Int
}
